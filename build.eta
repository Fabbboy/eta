let 
    # can't <str> + <str>
    # use concat operator `++`

    builtin = import "builtin"
    progs = builtin.progs
    
    work_dir = builtin.cwd
    src_dir = work_dir ++ "/eta"
    eta_bin = work_dir ++ "/eta.exe"

    odin_bin = builtin.find_program("odin")
    rm = progs.rm

    build = builtin.run([
        odin_bin.path(), "build", src_dir, "-out:{eta_bin}"
    ])
    
    clean = rm(eta_bin)
    run = builtin.run(eta_bin)
in 
 {build, clean, run}